{
  "name": "@form-observer/lit",
  "type": "module",
  "version": "0.7.2",
  "sideEffects": false,
  "description": "Convenience functions for the `@form-observer/core` package, designed for Lit apps",
  "exports": {
    ".": {
      "require": {
        "types": "./index.d.cts",
        "default": "./index.cjs"
      },
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./createFormValidityObserver": {
      "require": {
        "types": "./createFormValidityObserver.d.cts",
        "default": "./createFormValidityObserver.cjs"
      },
      "types": "./createFormValidityObserver.d.ts",
      "default": "./createFormValidityObserver.js"
    },
    "./automate": {
      "require": {
        "types": "./automate.d.cts",
        "default": "./automate.cjs"
      },
      "types": "./automate.d.ts",
      "default": "./automate.js"
    }
  },
  "peerDependencies": {
    "@form-observer/core": "^0.7.1",
    "lit": ">=3.0.0"
  },
  "author": "Isaiah Thomason",
  "license": "MIT",
  "keywords": [
    "form",
    "observer",
    "web",
    "validation",
    "lit",
    "progressive-enhancement"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enthusiastic-js/form-observer.git"
  },
  "bugs": {
    "url": "https://github.com/enthusiastic-js/form-observer/issues"
  },
  "homepage": "https://github.com/enthusiastic-js/form-observer#readme"
}
